{
  "name": "json-pretty-print",
  "version": "3.2.4",
  "description": "JSON Pretty Print Online",
  "author": "Jason Lee <jason@zerodevx.com>",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run minify",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "playwright test",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "minify": "html-minifier-terser -c minifier.json --file-ext html --input-dir docs/ --output-dir docs/"
  },
  "devDependencies": {
    "@fontsource/inter": "4.5.15",
    "@iconify-icons/line-md": "^1.2.22",
    "@iconify-icons/mdi": "1.2.44",
    "@iconify/svelte": "3.1.3",
    "@playwright/test": "1.33.0",
    "@sveltejs/adapter-static": "2.0.2",
    "@sveltejs/kit": "1.16.2",
    "@zerodevx/svelte-json-view": "1.0.5",
    "@zerodevx/svelte-toast": "0.9.3",
    "autoprefixer": "10.4.14",
    "copy-to-clipboard": "3.3.3",
    "daisyui": "2.51.6",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "html-minifier-terser": "7.2.0",
    "json5": "2.2.3",
    "postcss": "8.4.23",
    "postcss-load-config": "4.0.1",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.0",
    "svelte": "3.59.1",
    "svelte-check": "3.3.2",
    "svelte-preprocess": "5.0.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "zipurl": "0.2.15"
  },
  "type": "module",
  "license": "ISC",
  "repository": "github:zerodevx/json-pretty-print",
  "homepage": "https://zerodevx.github.io/json-pretty-print/",
  "keywords": [
    "json",
    "pretty-print",
    "online"
  ]
}
