{
  "name": "json-pretty-print",
  "version": "3.2.0",
  "description": "JSON Pretty Print Online",
  "author": "Jason Lee <jason@zerodevx.com>",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && html-minifier-terser -c minifier.json --file-ext html --input-dir docs/ --output-dir docs/",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "playwright test",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@fontsource/inter": "4.5.14",
    "@iconify-icons/mdi": "1.2.36",
    "@iconify/svelte": "3.0.1",
    "@playwright/test": "1.29.0",
    "@sveltejs/adapter-static": "1.0.0",
    "@sveltejs/kit": "1.0.1",
    "@tailwindcss/typography": "0.5.8",
    "@zerodevx/svelte-json-view": "1.0.0",
    "@zerodevx/svelte-toast": "1.0.0-rc.1",
    "autoprefixer": "10.4.13",
    "copy-to-clipboard": "3.3.3",
    "daisyui": "2.45.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "html-minifier-terser": "7.1.0",
    "json5": "2.2.2",
    "postcss": "8.4.20",
    "postcss-load-config": "4.0.1",
    "prettier": "2.8.1",
    "prettier-plugin-svelte": "2.9.0",
    "svelte": "3.55.0",
    "svelte-check": "2.10.2",
    "svelte-preprocess": "5.0.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "vite": "4.0.2",
    "zipurl": "0.2.14"
  },
  "type": "module",
  "license": "ISC",
  "repository": "github:zerodevx/json-pretty-print",
  "homepage": "https://zerodevx.github.io/json-pretty-print/",
  "keywords": [
    "json",
    "pretty-print",
    "online"
  ]
}
