{
  "name": "json-pretty-print",
  "version": "3.3.2",
  "description": "JSON Pretty Print Online",
  "author": "Jason Lee <jason@zerodevx.com>",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "npm run lint && vite build && npm run minify",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "playwright test",
    "lint": "prettier --check . && eslint . && npm run check",
    "format": "prettier --write .",
    "minify": "html-minifier-terser -c minifier.json --file-ext html --input-dir build/ --output-dir build/",
    "deploy": "npx gh-pages -d build -t -f"
  },
  "devDependencies": {
    "@fontsource-variable/inter": "^5.0.16",
    "@iconify-json/line-md": "^1.1.36",
    "@iconify-json/mdi": "^1.1.64",
    "@iconify/tailwind": "^0.1.4",
    "@playwright/test": "^1.41.2",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "^8.56.2",
    "@zerodevx/svelte-json-view": "^1.0.9",
    "@zerodevx/svelte-toast": "^0.9.5",
    "autoprefixer": "^10.4.17",
    "copy-to-clipboard": "^3.3.3",
    "daisyui": "^4.7.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "html-minifier-terser": "^7.2.0",
    "json5": "^2.2.3",
    "postcss": "^8.4.35",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "svelte": "^4.2.11",
    "svelte-check": "^3.6.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "zipurl": "^0.2.17"
  },
  "type": "module",
  "license": "ISC",
  "repository": "github:zerodevx/json-pretty-print",
  "homepage": "https://zerodevx.github.io/json-pretty-print/",
  "keywords": [
    "json",
    "pretty-print",
    "online"
  ]
}
